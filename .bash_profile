alias edit_bash_profile="vi ~/.bash_profile"
alias load_bash_profile="source ~/.bash_profile"
alias ping_yahoo="ping -c 4 yahoo.com"
alias curl_localhost="curl http://localhost"
alias edit_apache_config="sudo vi /etc/apache2/httpd.conf"
alias edit_hosts="sudo vi /etc/hosts"
alias edit_apache_vhosts="sudo vi /etc/apache2/extra/httpd-vhosts.conf"
alias tail_apache_log="tail -f /var/log/apache2/error_log"
alias restart_apache="sudo apachectl -k restart"
alias goto_webdevrecipes="cd /Library/WebServer/Documents/webdevrecipes/public_html"
alias edit_vimrc="vi ~/.vimrc"
alias extract_tar_gzipped="echo tar xzf [gzipped tar file name] -C [destination dir]"
